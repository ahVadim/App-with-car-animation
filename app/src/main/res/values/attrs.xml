<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="CarOnMapView">
        <attr name="showHintLine" format="boolean" />
        <attr name="carImage" format="reference" />
        <attr name="carWidth" format="dimension" />
        <attr name="turningRadius" format="dimension" />
        <attr name="carSpeed" format="enum">
            <enum name="slow" value="3"/>
            <enum name="normal" value="2"/>
            <enum name="fast" value="1"/>
        </attr>

    </declare-styleable>
</resources>
